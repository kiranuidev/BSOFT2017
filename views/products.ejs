<html>
 <head>
 <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootswatch/3.3.7/flatly/bootstrap.min.css"></head>
 <body>

 <div clas="container">
 <div class="row">
 <form method="post" action ="search">
 <div class="col-md-3">
    <input class="form-control" name="search">
 </div>
 <div class="col-md-3">
   <button type="submit" class="btn btn-primary"> Search</button>
 </div>
<div class="col-md-6">
 <ul class="pagination">
  <li><a href="/products/1">1</a></li>
  <li><a href="/products/2">2</a></li>
  <li><a href="/products/3">3</a></li>
  <li><a href="/products/4">4</a></li>
  <li><a href="/products/5">5</a></li>
</ul>
</div>
 </form>
</div>

 <table class="table table-hover">
    <thead>
      <tr>
        <th>S.No</th>
        <th>Product Model</th>
        <th>Description</th>
        <th>Price</th>
      </tr>
    </thead>
    <tbody>
    <% for(var i=0; i<products.length; i++) {%>
   

      <tr>
        <td><%= i+1 %></td>
        <td><%= products[i].Model  %></td>
        <td><%= products[i].Description  %></td>
        <td><%= products[i].Price  %></td>
      </tr>
      <% } %>
 </div>
           
 </body>
</html>